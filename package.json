{
  "name": "shared-components",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=2121 -o",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "prettier": "pretty-quick --staged",
    "build": "ng build",
    "build:git": "ng build --configuration=production --base-href=/angular/ --deploy-url=/angular/",
    "build:prod": "ng build --configuration=production",
    "build:test": "ng build --configuration=production --base-href=/ --deploy-url=/ --aot --output-hashing=all",
    "version_major": "npm --no-git-tag-version version major",
    "version_minor": "npm --no-git-tag-version version minor",
    "version_patch": "npm --no-git-tag-version version patch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.6",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.6",
    "@angular/material-moment-adapter": "^15.1.6",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@fortawesome/angular-fontawesome": "^0.12.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "moment": "^2.29.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.1",
    "@angular/cli": "~15.0.1",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.8.2"
  }
}
